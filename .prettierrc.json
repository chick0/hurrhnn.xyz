{
    "useTabs": false,
    "tabWidth": 4,
    "semi": true,
    "singleQuote": false,
    "trailingComma": "es5",
    "endOfLine": "lf",
    "bracketSameLine": true,
    "bracketSpacing": true,
    "arrowParens": "always",
    "proseWrap": "never",
    "printWidth": 120,
    "plugins": ["prettier-plugin-svelte"],
    "svelteStrictMode": true,
    "svelteAllowShorthand": false
}
